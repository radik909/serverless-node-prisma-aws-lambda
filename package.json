{
  "name": "chaos",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "start": "NODE_ENV=development serverless offline",
    "deploy": "NODE_ENV=production serverless deploy",
    "migrate": "dotenv -e .env.development -- npx prisma migrate dev",
    "generate": "dotenv -e .env.development -- npx prisma generate",
    "migrate:production": "dotenv -e .env.production -- npx prisma migrate deploy",
    "generate:production": "dotenv -e .env.production -- npx prisma generate",
    "prepare": "husky install",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@middy/core": "^2.5.3",
    "@middy/http-json-body-parser": "^2.5.3",
    "prisma": "^3.14.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@prisma/client": "^3.14.0",
    "@serverless/typescript": "^3.0.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/node": "^14.14.25",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "dotenv": "16.0.1",
    "dotenv-cli": "5.1.0",
    "esbuild": "^0.14.11",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.0",
    "json-schema-to-ts": "^1.5.0",
    "lint-staged": "^13.0.0",
    "prettier": "^2.6.2",
    "serverless": "^3.0.0",
    "serverless-dotenv-plugin": "^4.0.1",
    "serverless-esbuild": "^1.23.3",
    "serverless-offline": "^8.8.0",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "lint-staged": {
    "**/*.{js,ts}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
